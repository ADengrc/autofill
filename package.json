{
  "name": "autofill",
  "version": "1.0.0",
  "description": "自动填单工具",
  "main": "index.js",
  "build": {
    "appId": "com.example.app",
    "productName": "自动填单工具",
    "directories": {
      "buildResources": "build",
      "output": "dist"
    },
    "win": {
      "target": [
        {
          "target": "portable",
          "arch": [
            "x64"
          ]
        }
      ]
    }
  },
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder --win --x64"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "electron": "^8.5.2",
    "electron-builder": "^22.4.1"
  },
  "dependencies": {
    "moment": "^2.24.0",
    "puppeteer-core": "^2.1.1"
  }
}
